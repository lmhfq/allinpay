{
    "name": "lmh/allinpay",
    "description": "通联支付SDK",
    "authors": [
        {
            "name": "lmh",
            "email": "lmh@weiyian.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/log": "^1.0",
        "monolog/monolog": "^1.25.5",
        "pimple/pimple": "^3.0",
        "guzzlehttp/guzzle": "^6.5",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "ext-fileinfo": "*",
